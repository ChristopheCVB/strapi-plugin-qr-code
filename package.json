{
  "version": "0.4.3",
  "strapi": {
    "kind": "plugin",
    "name": "qr-code",
    "displayName": "QR-Code",
    "description": "A Strapi plugin to display generated QR Codes on the admin panel's ContentTypes."
  },
  "name": "strapi-plugin-qr-code",
  "description": "A Strapi plugin to display generated QR Codes on the admin panel's ContentTypes.",
  "keywords": [
    "strapi",
    "plugin",
    "qr-code"
  ],
  "author": {
    "name": "Christophe Carvalho Vilas-Boas",
    "url": "https://github.com/ChristopheCVB"
  },
  "maintainers": [
    {
      "name": "Christophe Carvalho Vilas-Boas",
      "url": "https://github.com/ChristopheCVB"
    }
  ],
  "contributors": [
    {
      "name": "Christophe Carvalho Vilas-Boas",
      "url": "https://github.com/ChristopheCVB"
    },
    {
      "name": "Vivian Grimaux",
      "url": "https://github.com/wezerg"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChristopheCVB/strapi-plugin-qr-code"
  },
  "license": "MIT",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "typecheck:front": "tsc --noEmit -p admin/tsconfig.json",
    "typecheck:back": "tsc --noEmit -p server/tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.2",
    "@strapi/icons": "^2.0.2",
    "qrcode.react": "^4.2.0",
    "react-intl": "^7.1.14",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@strapi/content-manager": "^5.33.1",
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/typescript-utils": "^5.33.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/koa": "^2.15.0",
    "@types/node": "^22.19.3",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.4.0",
    "@strapi/strapi": "^5.33.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19"
  },
  "engines": {
    "node": ">=22.21.1"
  },
  "packageManager": "pnpm@10.27.0",
  "volta": {
    "node": "22.21.1",
    "pnpm": "10.27.0"
  }
}
